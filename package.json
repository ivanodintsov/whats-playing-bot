{
  "name": "whats_playing_bot",
  "version": "0.0.1",
  "description": "Share, play, and add the Spotify songs to the queue just in one click. Let the magic happens.",
  "author": "Ivan Odyntsov",
  "private": true,
  "license": "GPL-3.0",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@apollo/gateway": "^2.0.5",
    "@nestjs/apollo": "^10.0.14",
    "@nestjs/bull": "^0.2.2",
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/graphql": "^10.0.15",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/microservices": "^7.6.0",
    "@nestjs/mongoose": "^7.0.2",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/serve-static": "^2.1.3",
    "@nestjs/terminus": "^7.1.0",
    "@nestjsplus/cookies": "^1.1.1",
    "@types/spotify-web-api-node": "^5.0.7",
    "@types/tunnel-ssh": "^4.1.1",
    "apollo-server-express": "^3.8.2",
    "bcrypt": "^5.0.0",
    "bull": "^3.18.0",
    "cache-manager": "^3.4.3",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.12.2",
    "cookie-parser": "^1.4.5",
    "graphql": "^16.5.0",
    "hbs": "^4.1.1",
    "mongoose": "^5.10.9",
    "nestjs-telegraf": "^2.4.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "ramda": "^0.27.1",
    "redis": "^3.1.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "spotify-web-api-node": "^5.0.2",
    "telegraf": "^4.8.2",
    "telegraf-ratelimit": "^2.0.0",
    "ts-morph": "^15.1.0",
    "tunnel-ssh": "^4.1.6",
    "typegram": "^3.9.1",
    "typescript": "4.4.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/bull": "^3.14.4",
    "@types/cache-manager": "^3.4.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.3",
    "@types/jest": "26.0.10",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^14.11.10",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/ramda": "^0.27.29",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "eslint": "7.7.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "26.4.2",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "9.0.0",
    "tsconfig-paths": "^3.9.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
